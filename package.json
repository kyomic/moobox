{
  "name": "@cookee/moobox",
  "version": "0.0.2",
  "type": "module",
  "files": [
    "lib"
  ],
  "main": "lib/index.js",
  "module": "./lib/index.js",
  "exports": {
    ".": {
      "import": "./lib/index.js",
      "require": "./lib/index.umd.cjs"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "build:lib": "vue-tsc --noEmit && vite build --mode=library",
    "preview": "vite preview",
    "build:wp": "webpack --config=webpack.config.js",
    "start:wp": "webpack serve --open",
    "publish": "npm run build:lib && npm publish --access public"
  },
  "dependencies": {
    "@tweenjs/tween.js": "^20.0.3",
    "@volar/vue-language-core": "1.6.3",
    "fast-glob": "^3.2.12",
    "less": "^4.1.3",
    "vite-plugin-svg-icons": "^2.0.1"
  },
  "devDependencies": {
    "@types/node": "^18.15.0",
    "@vitejs/plugin-vue": "^4.0.0",
    "clean-webpack-plugin": "^4.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.3",
    "html-webpack-plugin": "^5.5.0",
    "less-loader": "^11.1.0",
    "sass": "^1.59.2",
    "style-loader": "^3.3.2",
    "ts-loader": "^9.4.2",
    "typescript": "^4.9.5",
    "vite": "^4.1.0",
    "vite-plugin-compression": "^0.5.1",
    "vue": "^3.2.45",
    "vue-loader": "^17.0.1",
    "vue-router": "^4.0.13",
    "vue-tsc": "1.2.1",
    "vuex": "^4.0.2",
    "webpack": "^5.76.2",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.13.1"
  }
}
